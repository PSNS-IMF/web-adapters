<?xml version="1.0"?>
<package >
  <metadata>
    <id>Psns.Common.Web.Adapters</id>
    <version>$version$</version>
    <title>Psns Common Web Adapters</title>
    <authors>Jeff Purcell</authors>
    <owners>109.14</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Adapters that enable decoupling of existing System.Web components.</description>
    <projectUrl>https://github.com/PSNS-IMF/web-adapters</projectUrl>
    <licenseUrl>https://github.com/PSNS-IMF/web-adapters/blob/master/License</licenseUrl>
    <releaseNotes>
      ## 1.0.0.2
      ### Features
      * Adding support for .NET 4.5
    </releaseNotes>
    <copyright>Copyright 2014</copyright>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="Microsoft.AspNet.WebPages" version="2.0.30506.0" />
      </group>
      <group targetFramework="net45">
        <dependency id="Microsoft.AspNet.WebPages" version="3.2.3" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Web" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="lib\net40\*.*" target="lib\net40" />
    <file src="lib\net45\*.*" target="lib\net45" />
  </files>
</package>